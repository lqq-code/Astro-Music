{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_image.astro"
		},
		{
			"src": "^\\/$",
			"dest": "index.astro"
		},
		{
			"src": "^\\/api\\/musics\\/([^/]+?)$",
			"dest": "_id_.astro"
		},
		{
			"src": "^\\/api\\/musics$",
			"dest": "musics.astro"
		},
		{
			"src": "^\\/([^/]+?)\\/?$",
			"dest": "_id_.astro"
		}
	]
}